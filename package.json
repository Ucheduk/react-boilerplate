{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "A simple react boilerplate with unit test",
  "keywords": [
    "react-boilerplate",
    "boilerplate"
  ],
  "private": false,
  "main": "weback.config.js",
  "directories": {
    "test": "__tests__"
  },
  "verbose": true,
  "moduleNameMapper": {
    "^<components>/(.*)$": "<rootDir>/src/components/$1",
    "^<src>/(.*)$": "<rootDir>/src/$1",
    "^<validations>/(.*)$": "<rootDir>/src/validations/$1",
    "^<pages>/(.*)$": "<rootDir>/src/pages/$1",
    "^<layout>/(.*)$": "<rootDir>/src/components/layout/$1",
    "^<store>/(.*)$": "<rootDir>/src/store/$1",
    "^<helpers>/(.*)$": "<rootDir>/src/helpers/$1",
    "^<api>/(.*)$": "<rootDir>/src/api/$1",
    "^<organisms>/(.*)$": "<rootDir>/src/components/UI/organisms/$1",
    "^<templates>/(.*)$": "<rootDir>/src/components/templates/$1",
    "^<hooks>/(.*)$": "<rootDir>/src/hooks/$1",
    "^<image>/(.*)$": "<rootDir>/src/assets/images/$1"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "eslint \"src/**/*.js\"",
    "start": "node app.js",
    "build": "webpack --env.mode production",
    "dev": "webpack-dev-server  --env.mode development"
  },
  "author": {
    "name": "Ucheduk",
    "email": "uchenwakwuo@cre8tivetech.com"
  },
  "homepage": "https://github.com/Ucheduk/react-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ucheduk/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/Ucheduk/react-boilerplate/issues"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "favicons-webpack-plugin": "^3.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^25.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "parcel-bundler": "^1.12.4",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
